managed implementation in class zbp_rc_sorders_i unique;
strict ( 2 );

define behavior for ZRC_SORDERS_I alias Cubing
persistent table zrc_sorders
lock master
authorization master ( instance )
//etag master <field_name>
{
  create ( authorization : global );
  update;
  delete;
  field ( readonly ) Vbeln, Posnr;

  action ReleaseOrder result[1] $self;
