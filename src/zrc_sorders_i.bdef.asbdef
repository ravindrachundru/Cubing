  mapping for zrc_sorders
  {

  Vbeln = vbeln;
  Posnr = posnr;
  Matnr = matnr;
  Kwmeng = kwmeng;
  Vrkme = vrkme;
  Netpr = netpr;
  Netwr = netwr;
  Waerk = waerk;
  Kunnr = kunnr;
  Kunag = kunag;
  Erdat = erdat;
  Auart = auart;
  Vkorg = vkorg;
  Vtweg = vtweg;
  Spart = spart;
  Vdatu = vdatu;
  Werks = werks;
  Lgort = lgort;
  Augru = augru;
  Bstnk = bstnk;
  Openqty = openqty;
  Roundqty = roundqty;
  Itemfillrate = itemfillrate;
  Lfimg = lfimg;
  }
}